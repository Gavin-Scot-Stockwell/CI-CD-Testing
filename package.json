    "cypress": "^13.13.0",
    "eslint": "7.32.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.8.4" 
